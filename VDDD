local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "ToingHub",
   LoadingTitle = "Sabar Nyet..",
   LoadingSubtitle = "by tuwing",
   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil,
      FileName = "BySiriusHub"
   },
   Discord = {
      Enabled = false,
      Invite = "noinvitelink",
      RememberJoins = true
   },
   KeySystem = false,
   KeySettings = {
      Title = "Bu Sirius Key",
      Subtitle = "Key System",
      Note = "No key needed",
      FileName = "Key",
      SaveKey = false,
      GrabKeyFromSite = false,
      Key = {"Hello"}
   }
})

Rayfield:Notify({
   Title = "tuwing",
   Content = "hai memek??",
   Duration = 3,
   Image = 4483345998,
   Actions = {
      Ignore = {
         Name = "Okay!",
         Callback = function()
            print("User acknowledged notification")
         end
      },
   },
})

-- Tab Survivor
local SurvivorTab = Window:CreateTab("Survivor", 4483345998)
local SurvivorSection = SurvivorTab:CreateSection("Fitur")

local AutoGenButton = SurvivorTab:CreateButton({
   Name = "Auto Generator",
   Callback = function()
      print("Auto Generator activated")
      loadstring(game:HttpGet("https://raw.githubusercontent.com/trianaq765-cmd/VD/refs/heads/main/gene"))()
   end,
})

